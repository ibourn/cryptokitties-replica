  "source": "pragma solidity ^0.5.12;\r\n\r\n\r\n/**\r\n*  @dev -Allows to pause functions of the contract\r\n */\r\ncontract Pausable {\r\n\r\n    /**\r\n    *  @dev storage of the 'paused' state\r\n     */\r\n     bool internal _paused;\r\n\r\n    /**\r\n    *  @dev modifiers check the 'paused' state\r\n     */\r\n     modifier whenNotPaused() {\r\n         require(!_paused);\r\n         _;\r\n     }\r\n\r\n     modifier whenPaused() {\r\n         require(_paused);\r\n         _;\r\n     }\r\n}",
